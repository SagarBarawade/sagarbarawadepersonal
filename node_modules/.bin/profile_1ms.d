#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//usr/sbin/dtrace" ]; then
  "$basedir//usr/sbin/dtrace"  -s "$basedir/../perf-sym/profile_1ms.d" "$@"
  ret=$?
else 
  /usr/sbin/dtrace  -s "$basedir/../perf-sym/profile_1ms.d" "$@"
  ret=$?
fi
exit $ret
